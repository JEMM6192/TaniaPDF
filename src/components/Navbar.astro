---
import logo from "../assets/logo2.webp";
---

<nav
  class="navbar bg-base-100 p-4 sm:border-b border-base-content/25 sm:z-10 relative"
>
  <button
    type="button"
    class="btn btn-text sm:hidden me-2"
    aria-haspopup="dialog"
    aria-expanded="false"
    aria-controls="with-navbar-sidebar"
    data-overlay="#with-navbar-sidebar"
  >
    <span class="icon-[tabler--menu-2] size-5"></span>
  </button>
  <div class="flex-1 flex justify-center items-center">
    <img src={logo.src} alt="Logo" class="h-12 w-auto" />
  </div>
</nav>
